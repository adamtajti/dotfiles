{
	"name": "adamtajti-snippets",
	"engines": {
		"vscode": "^1.11.0"
	},
	"contributes": {
		"snippets": [
			{
				"language": [
					"shellscript",
					"shell",
					"markdown",
					"sh",
					"zsh",
					"yaml"
				],
				"path": "./snippets/shell.json"
			},
			{
				"language": [
					"yaml",
					"yml"
				],
				"path": "./snippets/yaml.json"
			},
			{
				"language": [
					"javascript",
					"typescript"
				],
				"path": "./snippets/typescript.json"
			},
			{
				"language": [
					"nix"
				],
				"path": "./snippets/nix.json"
			},
			{
				"language": [
					"json"
				],
				"path": "./snippets/json.json"
			},
			{
				"language": [
					"all"
				],
				"path": "./snippets/all.json"
			}
		]
	}
}
