{
  "An .editorconfig file example": {
    "prefix": ["p-example"],
    "body": [
      "root = true",
      "[*]",
      "end_of_line = lf",
      "insert_final_newline = true",
      "indent_style = space",
      "indent_size = 2"
    ]
  },
  "Sets the max line length aka the width": {
    "prefix": ["p-max-line-length-or-width"],
    "body": ["max_line_length = 120"]
  }
}
